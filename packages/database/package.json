{
  "name": "@hndl/database",
  "version": "1.0.0",
  "description": "",
  "main": "src/client.ts",
  "type": "module",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "dev": "tsc",
    "format": "prettier . --write",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrade dev",
    "db:studio": "prisma studio"
  },
  "exports": {
    ".": {
      "types": "./src/client.ts",
      "default": "./dist/src/client.js"
    },
    "./enums": {
      "types": "./generated/prisma/enums.ts",
      "default": "./dist/generated/prisma/enums.js"
    },
    "./client": {
      "types": "./generated/prisma/client.ts",
      "default": "./dist/generated/prisma/client.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@hndl/eslint-config": "workspace:*",
    "@hndl/prettier-config": "workspace:^",
    "@hndl/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "@types/pg": "^8.16.0",
    "dotenv": "^17.2.3",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "prisma": "^7.3.0",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "pg": "^8.18.0"
  }
}
