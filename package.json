{
  "name": "turbolistan",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "typecheck": "turbo run typecheck",
    "db:push": "turbo run --filter='@hndl/database' pnpm run db:push",
    "db:generate": "turbo run --filter='@hndl/database' pnpm run db:generate",
    "db:migrate": "turbo run --filter='@hndl/database' pnpm run db:migrate"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "prettier": "catalog:",
    "turbo": "^2.7.1",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.26.1",
  "engines": {
    "node": ">=18"
  },
  "license": "MIT"
}
